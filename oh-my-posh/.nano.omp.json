{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "style": "plain",
          "template": "\udb80\ude4b {{ .Path }} ",
          "properties": {
            "style": "folder"
          },
          "foreground": "blue"
        },
        {
          "type": "git",
          "style": "plain",
          
          "properties": {
            "fetch_status": true,
            "branch_icon": "\udb81\ude2c "
          },
          "foreground": "red"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "yellow",
          "template": "‚ùØ"
        }
      ]
    }
  ]
}